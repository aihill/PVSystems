within PVSystems;
package Icons "Library of icons"
extends Modelica.Icons.IconsPackage;


  annotation(Documentation(info="
    <html>
      <p>
        This package contains definitions for the graphical layout of
        components which may be used in different libraries. The icons can
        be utilized by inheriting them in the desired class using
        \"extends\" or by directly copying the \"icon\" layer.</p>
    </html>"));
end Icons;
